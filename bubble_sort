def  sort(l):
    for i in range(n):
        for j in range(0,n-i-1):
            if l[j] > l[j+1] : 
                l[j], l[j+1] = l[j+1], l[j] 







n=int(input("enter a size:"))  #getting value from runtime
l=[]
for k in range(n):
    e=int(input())
    l.append(e)
    
sort(l)                        #method calling
print("the sorted")

for i in range(n):             #printing the sorted list
    print(l[i],end=" ")

        
